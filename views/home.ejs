    <style>
        body {
            margin: 0;
            font-family: 'Roboto', sans-serif;
            background-color: #121212;
            color: #ffffff;
        }

        header {
            background-color: #1f1f1f;
            padding: 20px;
            display: flex;
            justify-content: space-between;
            align-items: center;
        }

        header h1 {
            margin: 0;
            font-size: 24px;
        }

        nav ul {
            list-style: none;
            padding: 0;
            margin: 0;
            display: flex;
        }

        nav ul li {
            margin-left: 20px;
            transition: transform 0.3s;
        }

        nav ul li:hover {
            transform: scale(1.1);
        }

        nav ul li a {
            color: #ffffff;
            text-decoration: none;
            font-size: 18px;
        }

        .search-bar {
            display: flex;
            align-items: center;
        }

        .search-bar input {
            color:#070707;
            padding: 10px;
            border: none;
            border-radius: 5px;
            margin-right: 10px;
        }

        .search-bar button {
            padding: 10px;
            border: none;
            border-radius: 5px;
            background-color: #ff5722;
            color: #ffffff;
            cursor: pointer;
        }

        .categories {
            display: flex;
            justify-content: center;
            margin: 20px 0;
        }

        .categories a {
            margin: 0 10px;
            padding: 10px 20px;
            background-color: #333333;
            border-radius: 5px;
            text-decoration: none;
            color: #ffffff;
        }

        .wallpapers {
            column-count: 3;
            column-gap: 1rem;
            padding: 20px;
        }

        .wallpaper {
            margin-bottom: 1rem;
            border-radius: 10px;
            overflow: hidden;
            position: relative;
            break-inside: avoid;
            transition: transform 0.3s;
        }

        .wallpaper:hover {
            transform: scale(1.05);
        }

        .wallpaper img {
            width: 100%;
            height: auto;
            display: block;
        }

        .wallpaper .overlay {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: rgba(0, 0, 0, 0.5);
            display: flex;
            justify-content: center;
            align-items: center;
            opacity: 0;
            transition: opacity 0.3s;
        }

        .wallpaper:hover .overlay {
            opacity: 1;
        }

        .overlay a {
            color: #ffffff;
            font-size: 24px;
            text-decoration: none;
            margin: 0 10px;
        }

        @media (max-width: 768px) {
            .wallpapers {
                column-count: 2;
            }
        }

        @media (max-width: 480px) {
            .wallpapers {
                column-count: 1;
            }
        }
    </style>
</head>

